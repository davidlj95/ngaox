{
  "root": "docs",
  "sourceRoot": "docs/content",
  "projectType": "application",
  "targets": {
    "build-content": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npx ts-node --project docs/build/tsconfig.json -r tsconfig-paths/register -T docs/build"
        ],
        "parallel": false
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["npx nx build-content docs", "npx nx build docs-app"],
        "parallel": false
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npx nx build-content docs --watch",
          "npx nx serve docs-app"
        ],
        "parallel": true
      }
    }
  },
  "tags": []
}

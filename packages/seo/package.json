{
  "name": "@ngaox/seo",
  "version": "1.5.1",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "cp --parents schematics/*/schema.json ../../dist/packages/seo/",
    "copy:files": "cp --parents -p -r schematics/*/files/** ../../dist/packages/seo/",
    "copy:collection": "cp schematics/collection.json ../../dist/packages/seo/schematics/collection.json",
    "postbuild": "yarn copy:schemas && yarn copy:files && yarn copy:collection"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "description": "Angular library to help generate & managing Social-Media/SEO meta tags, canonical links & page title.",
  "peerDependencies": {
    "@angular/common": "^13.1.0",
    "@angular/core": "^13.1.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rabraghib/ngaox.git"
  },
  "keywords": [
    "ngaox",
    "seo",
    "ngx",
    "angular",
    "universal",
    "ssr"
  ],
  "author": "Raby√¢ Raghib",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rabraghib/ngaox/issues"
  },
  "homepage": "https://github.com/rabraghib/ngaox/tree/main/packages/seo"
}

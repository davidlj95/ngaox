{
  "name": "@ngaox/icons",
  "version": "1.0.1",
  "description": "Easily inline SVG icons into your angular app with a ready to import and use set of icons",
  "bin": {
    "ngaox-icons": "bin/bundle.js"
  },
  "scripts": {
    "bundle-bin": "../../node_modules/.bin/webpack -c ./webpack.config.js --mode production",
    "build": "yarn bundle-bin && node bin/bundle.js build src/svgs src/svgs/index.json"
  },
  "peerDependencies": {
    "@angular/common": "^12.1.2",
    "@angular/core": "^12.1.2"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rabraghib/ngaox.git"
  },
  "keywords": [
    "ngaox",
    "icons",
    "ngx",
    "angular",
    "svg",
    "ssr"
  ],
  "author": "Raby√¢ Raghib",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rabraghib/ngaox/issues"
  },
  "homepage": "https://github.com/rabraghib/ngaox/tree/main/packages/icons"
}
